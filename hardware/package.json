{
  "name": "paradrone-hardware",
  "description": "ParaDrone hardware CAD designs",
  "license": "MIT",
  "dependencies": {
    "@jscad/cli": "^2.2.26",
    "@jscad/modeling": "^2.11.1"
  },
  "scripts": {
    "model": "jscad jscad/index.js -o model.obj",
    "print": "jscad jscad/index.js -o print.stl --print=1",
    "actuator": "jscad jscad/actuator.js -o actuator.stl --print=1",
    "autopilot": "jscad jscad/autopilot.js -o autopilot.stl --print=1",
    "battery": "jscad jscad/battery.js -o battery.stl --print=1",
    "remotecontrol": "jscad jscad/remotecontrol.js -o remotecontrol.stl --print=1",
    "spool": "jscad jscad/spool.js -o spool.stl --print=1"
  }
}
