{
  "name": "paradrone-hardware",
  "description": "ParaDrone hardware CAD designs",
  "license": "MIT",
  "dependencies": {
    "@jscad/cli": "^2.2.20",
    "@jscad/modeling": "^2.9.4"
  },
  "scripts": {
    "model": "npx jscad jscad/index.js -o model.obj",
    "print": "npx jscad jscad/index.js -o print.stl --print=1",
    "actuator": "npx jscad jscad/actuator.js -o actuator.stl --print=1",
    "autopilot": "npx jscad jscad/autopilot.js -o autopilot.stl --print=1",
    "battery": "npx jscad jscad/battery.js -o battery.stl --print=1",
    "remotecontrol": "npx jscad jscad/remotecontrol.js -o remotecontrol.stl --print=1",
    "spool": "npx jscad jscad/spool.js -o spool.stl --print=1"
  }
}
